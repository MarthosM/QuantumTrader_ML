# Configurações do projeto ML Trading v2.0

# Database
DATABASE_URL=sqlite:///data/trading.db

# API Keys (exemplo - substitua pelos valores reais)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here
POLYGON_API_KEY=your_polygon_key_here

# Trading Parameters
DEFAULT_TIMEFRAME=1H
LOOKBACK_PERIOD=100
PREDICTION_HORIZON=1

# Model Parameters
RANDOM_STATE=42
TEST_SIZE=0.2
VALIDATION_SIZE=0.2

# Optuna Parameters
N_TRIALS=100
OPTUNA_TIMEOUT=3600  # 1 hour

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/trading.log
